model Job {
  id             String   @id @default(uuid())
  position       String   
  company        String   
  location       String   
  description    String?  
  salary         String?  
  jobUrl         String
  jobPostedDate  String
  jobAgoTime     String
  companyLogo    String?  
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  pineconeId     String?
}
